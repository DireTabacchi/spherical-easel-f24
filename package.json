{
  "name": "spherical-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run app:serve && npm run docs:serve",
    "build": "npm run app:build && npm run docs:build",
    "app:serve": "vue-cli-service serve",
    "app:build": "vue-cli-service build",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress dev docs --debug --open",
    "test:unit": "MODE=test vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/languages/**/*.json'"
  },
  "dependencies": {
    "@types/color": "^3.0.1",
    "@types/two.js": "^0.7.2",
    "core-js": "^3.6.4",
    "moment": "^2.26.0",
    "three": "^0.117.1",
    "two.js": "^0.7.0-stable.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.18.2",
    "vue-property-decorator": "^8.5.0",
    "vue-router": "^3.3.4",
    "vue-splitpane": "^1.0.6",
    "vuetify": "^2.3.1",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@mdi/js": "^5.3.45",
    "@types/jest": "^24.0.19",
    "@types/two.js": "^0.7.2",
    "@types/vue-splitpane": "^1.0.0",
    "@types/webpack": "^4.41.17",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@vue/cli-plugin-babel": "~4.4.4",
    "@vue/cli-plugin-eslint": "~4.4.4",
    "@vue/cli-plugin-router": "~4.4.4",
    "@vue/cli-plugin-typescript": "~4.4.4",
    "@vue/cli-plugin-unit-jest": "~4.4.4",
    "@vue/cli-plugin-vuex": "~4.4.4",
    "@vue/cli-service": "~4.4.4",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.3",
    "@vuepress/plugin-back-to-top": "^1.5.1",
    "canvas": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "gl": "^4.5.0",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.5",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2",
    "vuepress-plugin-mathjax": "^1.2.8",
    "vuetify-loader": "^1.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
